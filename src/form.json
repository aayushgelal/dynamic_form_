{
  "contact": {
    "fields": [
      {
        "id": "name",
        "type": "text",
        "label": "Name",
        "required": true,
        "placeholder": "Enter your name",
        "validation": {
          "required": true,
          "minLength": 3,
          "maxLength": 20
        },
        "errorMessages": {
          "required": "Name is required",
          "minLength": "Name must be at least 3 characters",
          "maxLength": "Name cannot exceed 20 characters"
        }
      },
      {
        "id": "email",
        "type": "email",
        "label": "Email",
        "required": true,
        "placeholder": "Enter your email",
        "validation": {
          "required": true,
          "pattern": "^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$"
        },
        "errorMessages": {
          "required": "Email is required",
          "pattern": "Please enter a valid email address"
        }
      },
      {
        "id": "message",
        "type": "textarea",
        "label": "Message",
        "required": true,
        "placeholder": "Enter your message",
        "validation": {
          "required": true
        },
        "errorMessages": {
          "required": "Message is required"
        }
      },
      {
        "id": "file",
        "type": "file",
        "label": "File",
        "required": true,
        "placeholder": "Upload your file",
        "validation": {
          "required": true
        },
        "errorMessages": {
          "required": "File upload is required"
        }
      }
    ],
    "url": "/contact"
  },
